            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name_Team)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Racing_level)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Points_in_seasion)
        </td>
        <td>
